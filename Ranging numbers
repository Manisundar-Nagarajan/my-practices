// To range the number between A and B
#include<stdio.h>
int main()
{
        int m1, m2;
        printf("Enter the values: ");
        scanf("%d %d", &m1, &m2);
        if(m1<m2)
        {
                while(m1<=m2)
                {       printf("%d ", m1);
                m1++;
                }
        }
        else if(m1>m2)
        {
                while(m1>=m2)
                {
                        printf("%d ", m1);
                m1--;
                }
        }
        else
                printf("invalid number\n");
}
