//To display the Fibonacci series
#include<stdio.h>
int main()
{
        int x=0, y=1, n, count=1, z;
        printf("Enter the value\n");
        scanf("%d", &n);
        while(count<=n)
        {
                printf("%d ",x);
                z=x+y;
                x=y;
                y=z;
                count++;
        }
        printf("\n");
}
