//To find the given number is a prime number or not
#include<stdio.h>
int main()
{
int num, i, count=0;
printf("Enter the number: ");
scanf("%d", &num);
if(num<=1)
    printf("%d is not a prime number\n", num);
else
    {
    for(i=2;i<=num;i++)
    {
        if(num%i==0)
            count++;
    }
    }
    if(count==1)
        printf("%d is a prime number", num);
    else if(count>=1)
    printf("%d is not a prime number\n", num);
   return 0;
}
