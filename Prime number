//To display 50th prime number 
#include<stdio.h>
int main()
{
        int num, count=0, i;
        for(num=1;count<100;num++)
        {
                for(i=2;i<=num/2;i++)
                {
                        if(num%i==0)
                                break;
                }
                if(i==(num/2)+1)
                {
                count++;
                }
                if(count==50)
                {
                        printf(" The 50th prime number is: %d\n", num);
                break;
                }
        }
        return 0;
}






