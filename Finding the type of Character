// To find the given character
int main()
{
        char ch;
        printf("The given character is:");
        scanf("%c",&ch);
        printf("the ASCII valve of the given character %c is: %d\n", ch, ch);
        if(ch>=65 && ch<=90)
                printf("The given data is an uppercase alphabet\n");
        else if(ch>=97 && ch<=122)
                printf("The given data is an Lowercase alphabet\n");
        else if(ch>=48 && ch<=57)
                printf("The given data is an integer\n");
        else
                printf("The given data is a special character\n");
