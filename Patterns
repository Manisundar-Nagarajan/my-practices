#include <stdio.h>
int main()
{
    int i, j, n, k;
    printf("Enter the number of rows: ");
    scanf("%d", &n);
    for(i=0;i<n;i++)
    {
        if(i%2==0)
            k=1;
        else
            k=2;
        for(j=0;j<(i+1);j++)
        {
            printf("%d ", k);
            k=k+2;
        }
        printf("\n");
    }
}
Output:
Enter the number of rows: 5
1
2 4
1 3 5
2 4 6 8
1 3 5 7 9
