//To find the given number is a prime number or not
#include<stdio.h>
int main()
{
int num, i;
printf("Enter the number: ");
scanf("%d", &num);
for(i=2;i<=num/2;i++)
    {
        if(num%i==0)
            break;
    }
if(i=num/2+1)
printf("%d is a prime number\n", num);
else 
printf("%d is not a prime number\n", num);
   return 0;
}
