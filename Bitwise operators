// To check whether the gven number is power of 2 or not//
#include<stdio.h>
int main()
{
        int num;
        printf("Enter the number: ");
        scanf("%d", &num);
        if((num&(num-1))==0)
                printf("Given number is a power of 2\n");
        else
        printf("Its not a power of 2\n");
}



