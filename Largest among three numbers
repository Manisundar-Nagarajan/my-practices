// To find the largest amoung three numbers
#include<stdio.h>
int main()
{
int a, b, c;
printf("The value of a= ");
scanf("%d", &a);
printf("The value of b= ");
scanf("%d", &b);
printf("The value of c= ");
scanf("%d", &c);
if(a>b && a>c)
printf("a=%d is the largest value", a);
else if(b>a && b>c)
printf("b=%d is the largest value", b);
else if(c>a && c>b)
printf("c=%d is the largest value", c);
else if(a==b && c>a)
printf("c=%d is the largest value", c);
else if(c==b && a>b)
printf("a=%d is the largest value", a);
else if(a==c && b>c)
printf("%b=d is the largest value", b);
else if(a==b && c<a)
printf("a=%d b=%d are the largest values", a, b);
else if(c==b && a<b)
printf("c=%d b=%d are the largest values", c, b);
else if(a==c && b<c)
printf("a=%d c=%d are the largest values", a, c);
else
printf("All are same value");
return 0;
}
