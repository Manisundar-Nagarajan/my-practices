// To check the given number is an armstrong number or not
#include<stdio.h>
int main()
{
        int n, sum, m, rev, rem=0;
        printf("Enter the value: ");
        scanf("%d", &n);
        m=n;
        while(m!=0)
        {
                sum=1;
                rev=m%10;
                while(rev!=0)
                {
                sum=sum*rev;
                rev--;
                }
                rem+=sum;
                m=m/10;
        }
        if(n==rem)
                printf("The given number %d is a strong number\n", n);
        else
                printf("Its not a strong number\n");
        return 0;
}
