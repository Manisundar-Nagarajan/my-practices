// To display the multiplication table of a given number
#include<stdio.h>
int main()
{
        int x=1, n;
        printf(" the Multiplication table for: ");
        if(scanf("%d", &n) !=1)
        {
                printf("The given number is not an integer");
        return 1;
        }
        mlt:
        printf("%d*%d=%d\n", x, n, x*n);
        x++;
        if(x<=10)
        goto mlt;
}
